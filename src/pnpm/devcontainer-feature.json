{
    "name": "Pnpm (via npm)",
    "id": "pnpm",
    "version": "1.0.0",
    "description": "Fast, disk space efficient package manager.",
    "documentationURL": "https://github.com/NicoVIII/devcontainer-features/tree/main/src/pnpm",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "7.14.2",
                "next-7",
                "latest-6"
            ],
            "default": "latest",
            "description": "Select the version of Pnpm to install."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/node"
    ]
}
